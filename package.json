{
  "name": "rss-sponsorblock",
  "version": "1.0.0",
  "description": "Self-hosted podcast ad remover using Gemini 2.5 Flash",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "tsx --watch src/index.js",
    "dev:build": "npm run build && node dist/index.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test tests/*.test.js",
    "test:watch": "tsx --test --watch tests/*.test.js",
    "test:ts": "tsx --test tests/*.test.ts",
    "test:node": "node --test tests/*.test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/generative-ai": "^0.19.0",
    "axios": "^1.10.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "xml2js": "^0.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/express": "^5.0.3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^24.0.4",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
